<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qf.j1904.mapper.CityMapper_xpy">
    <select id="findCityById" parameterType="int" resultType="hi.car.pojo.City">
        select
          *
        from
          city
        where
          id=#{id}
    </select>
</mapper>